{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "module": "ES2022",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "types": ["node"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "target": "ESNext",
    "allowJs": true,
    "resolveJsonModule": true,
    "moduleDetection": "force",
    "isolatedModules": true,
    "verbatimModuleSyntax": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitOverride": true,
    "noEmit": true,
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "moduleResolution": "Bundler",
    "useDefineForClassFields": true,
    "noImplicitThis": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "nitropack/types": ["../node_modules/nitropack/types"],
      "nitropack/runtime": ["../node_modules/nitropack/runtime"],
      "nitropack": ["../node_modules/nitropack"],
      "defu": ["../node_modules/defu"],
      "h3": ["../node_modules/h3"],
      "consola": ["../node_modules/consola"],
      "ofetch": ["../node_modules/ofetch"],
      "@unhead/vue": ["../node_modules/@unhead/vue"],
      "vue": ["../node_modules/vue"],
      "@vue/runtime-core": ["../node_modules/@vue/runtime-core"],
      "@vue/compiler-sfc": ["../node_modules/@vue/compiler-sfc"],
      "vue-router": ["../node_modules/vue-router"],
      "vue-router/auto-routes": ["../node_modules/vue-router/vue-router-auto-routes"],
      "unplugin-vue-router/client": ["../node_modules/unplugin-vue-router/client"],
      "@nuxt/schema": ["../node_modules/@nuxt/schema"],
      "nuxt": ["../node_modules/nuxt"],
      "vite/client": ["../node_modules/vite/client"],
      "~": [".."],
      "~/*": ["../*"],
      "@": [".."],
      "@/*": ["../*"],
      "~~": [".."],
      "~~/*": ["../*"],
      "@@": [".."],
      "@@/*": ["../*"],
      "#shared": ["../shared"],
      "#shared/*": ["../shared/*"],
      "#app": ["../node_modules/nuxt/dist/app"],
      "#app/*": ["../node_modules/nuxt/dist/app/*"],
      "vue-demi": ["../node_modules/nuxt/dist/app/compat/vue-demi"],
      "#ui": ["../node_modules/@nuxt/ui/dist/runtime"],
      "#ui/*": ["../node_modules/@nuxt/ui/dist/runtime/*"],
      "#ui-colors": ["./ui.colors"],
      "#color-mode-options": ["./color-mode-options.mjs"],
      "#vue-router": ["../node_modules/vue-router"],
      "#unhead/composables": ["../node_modules/nuxt/dist/head/runtime/composables"],
      "#imports": ["./imports"],
      "#tailwind-config": ["./tailwind/expose"],
      "#tailwind-config/*": ["./tailwind/expose/*"],
      "#app-manifest": ["./manifest/meta/7d8b0bbe-c49a-4bb3-b9bf-892571ef41c1.json"],
      "#components": ["./components"],
      "#build": ["."],
      "#build/*": ["./*"]
    }
  },
  "include": [
    "./nuxt.d.ts",
    "../**/*",
    "../modules/*/runtime/**/*",
    "../layers/*/app/**/*",
    "../layers/*/modules/*/runtime/**/*",
    "../shared/**/*.d.ts",
    "../modules/*/shared/**/*.d.ts",
    "../layers/*/shared/**/*.d.ts",
    "../*.d.ts",
    "../layers/*/*.d.ts",
    "./nuxt.node.d.ts",
    "../modules/*.*",
    "../nuxt.config.*",
    "../.config/nuxt.*",
    "../layers/*/nuxt.config.*",
    "../layers/*/.config/nuxt.*",
    "../layers/*/modules/**/*",
    "../shared/**/*",
    "../modules/*/shared/**/*",
    "../layers/*/shared/**/*",
    "../node_modules/runtime",
    "../node_modules/dist/runtime",
    "../types/**/*"
  ],
  "exclude": [
    "../node_modules",
    "../node_modules/nuxt/node_modules",
    "../node_modules/@nuxt/icon/node_modules",
    "../node_modules/@nuxtjs/color-mode/node_modules",
    "../node_modules/@nuxtjs/tailwindcss/node_modules",
    "../node_modules/@nuxt/ui/node_modules",
    "../node_modules/@pinia/nuxt/node_modules",
    "../node_modules/@sentry/nuxt/node_modules",
    "../node_modules/@nuxt/devtools/node_modules",
    "../node_modules/@nuxt/telemetry/node_modules",
    "../dist",
    "../.data",
    "../modules/*/runtime/server/**/*",
    "../layers/*/server/**/*",
    "../layers/*/modules/*/runtime/server/**/*",
    "../node_modules/runtime/server",
    "../node_modules/dist/runtime/server",
    "venv",
    "**/venv/**",
    // Exclude large source and data folders that are not actively edited to speed up the TS/JS language service
    "../data",
    "../matlab",
    "../mobiles-dataset-docs",
    "../python_api",
    "../public",
    "../screenshots",
    "../test-results",
    "../tests",
    "../docs",
    "../deployment",
    "../examples",
    "../instantsearch-app",
    "../playwright-report",
    "../wiki"
  ]
}
