{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ES2022",
    "moduleResolution": "bundler",
    "noEmit": true,
    "allowJs": true,
    "strict": true,
    "types": ["node"]
  },
  "include": ["server/**/*", "pages/**/*", "components/**/*"],
  "exclude": ["node_modules", ".nuxt", ".output"]
}
