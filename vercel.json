{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": ".output/public"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ],
    "functions": {
        "server/**/*.js": {
            "runtime": "nodejs18.x"
        },
        "server/**/*.ts": {
            "runtime": "nodejs18.x"
        }
    },
    "regions": [
        "fra1"
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "framework": "nuxt"
}