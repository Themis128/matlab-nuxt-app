{
        "name": "Nuxt + Python Dev Container",
        "image": "mcr.microsoft.com/devcontainers/javascript-node:22",
        "workspaceFolder": "/workspace",
        "workspaceMount": "source=${localWorkspaceFolder},target=/workspace,type=bind,consistency=cached",
        "features": {
          "ghcr.io/devcontainers/features/python": {
            "version": "3.14"
          },
          "ghcr.io/devcontainers/features/git": {},
          "ghcr.io/devcontainers/features/docker-in-docker": {}
        },
        "customizations": {
          "vscode": {
            "extensions": [
              "ms-vscode.vscode-typescript-next",
              "vue.volar",
              "ms-python.python",
              "ms-python.vscode-pylance",
              "ms-playwright.playwright",
              "esbenp.prettier-vscode",
              "dbaeumer.vscode-eslint",
              "eamodio.gitlens",
              "ms-azuretools.vscode-docker",
              "streetsidesoftware.code-spell-checker",
              "octref.vetur"
            ],
            "settings": {
              "typescript.tsdk": "node_modules/typescript/lib",
              "typescript.enablePromptUseWorkspaceTsdk": true,
              "javascript.suggest.enabled": true,
              "typescript.suggest.enabled": true,
              "editor.formatOnSave": true,
              "editor.codeActionsOnSave": {
                "source.fixAll.eslint": true
              },
              "eslint.validate": [
                "javascript",
                "typescript",
                "vue"
              ],
              "files.exclude": {
                "**/node_modules": true,
                "**/.git": true,
                "**/.venv": true
              }
            }
          }
        },
        "postCreateCommand": "bash -lc 'npm ci && npx playwright install --with-deps && cd python_api && python -m pip install --upgrade pip && pip install -r requirements.txt && sudo apt-get update && sudo apt-get install -y awscli tmux || true && npm install -g @sentry/cli || true && chmod +x .devcontainer/health-check.sh .devcontainer/start-dev.sh .devcontainer/start-servers-tmux.sh scripts/shell/start.sh'",
        "postStartCommand": "bash -lc './.devcontainer/health-check.sh; if [ \"$DEVCONTAINER_AUTO_START_SERVERS\" = \"true\" ]; then ./.devcontainer/start-servers-tmux.sh || true; fi; echo Devcontainer ready. Ports forwarded: 3000 (frontend), 8000 (Python API)'",
        "containerEnv": {
          "DEVCONTAINER_AUTO_START_SERVERS": "true"
        },
        "forwardPorts": [
          3000,
          8000,
          9229
        ],
        "remoteUser": "vscode",
        "shutdownAction": "stopContainer",
        "overrideCommand": false
      }
