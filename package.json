{
  "name": "matlab-nuxt-app",
  "version": "1.0.0",
  "description": "A comprehensive MATLAB and Nuxt.js project for deep learning experiments, mobile phone dataset analysis, and MATLAB capabilities checking",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "dev:python": "cd python_api && python api.py",
    "dev:all": "concurrently -t -n PY,NUXT -c blue,green \"cd python_api && ..\\venv\\Scripts\\python api.py\" \"nuxt dev\"",
    "typecheck": "nuxi typecheck || vue-tsc --noEmit --skipLibCheck && tsc -p tsconfig.json --noEmit",
    "lint": "eslint . --ext .js,.ts,.vue --max-warnings=0",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "check": "node check-capabilities.js",
    "check:matlab": "node check-capabilities.js",
    "next-steps": "node run-next-steps.js",
    "test": "set NUXT_TEST=1&& playwright test",
    "test:ui": "playwright test --ui",
    "test:report": "playwright show-report",
    "mat:view": "node view_mat_file.js"
  },
  "dependencies": {
    "@nuxt/ui": "^2.19.0",
    "@pinia/nuxt": "^0.11.3",
    "apexcharts": "^5.3.6",
    "nuxt": "^4.2.1",
    "pinia": "^3.0.4",
    "vue3-apexcharts": "^1.10.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@playwright/test": "^1.56.1",
    "@types/node": "^24.10.0",
    "concurrently": "^9.0.1",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.28.0",
    "@typescript-eslint/eslint-plugin": "^8.12.1",
    "@typescript-eslint/parser": "^8.12.1",
    "typescript": "^5.9.3",
    "vue-tsc": "^3.1.3"
  },
  "overrides": {
    "glob": "10.1.0",
    "@vercel/nft": {
      "glob": "10.1.0"
    },
    "archiver-utils": {
      "glob": "10.1.0"
    },
    "replace-in-file": {
      "glob": "10.1.0"
    },
    "sucrase": {
      "glob": "10.1.0"
    }
  },
  "keywords": [
    "matlab",
    "nuxt",
    "deep-learning",
    "neural-networks",
    "machine-learning",
    "capabilities",
    "toolboxes",
    "mobile-phones",
    "dataset-analysis",
    "cnn",
    "lstm",
    "autoencoder"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Themis128/matlab-nuxt-app.git"
  },
  "bugs": {
    "url": "https://github.com/Themis128/matlab-nuxt-app/issues"
  },
  "homepage": "https://github.com/Themis128/matlab-nuxt-app#readme"
}
