# Copy this file to .env and update values for your environment
# NOTE: Don't commit your real secrets.

# Python API
PYTHON_UNBUFFERED=1
REDIS_URL=redis://redis:6379
# Port mapping used by python-api image
PYTHON_PORT=8000

# Nuxt (frontend)
NODE_ENV=production
NUXT_PUBLIC_API_BASE=http://python-api:8000
NUXT_API_SECRET=changeme

# Algolia (optional)
ALGOLIA_APP_ID=your_algolia_app_id
ALGOLIA_ADMIN_API_KEY=your_algolia_admin_api_key
ALGOLIA_INDEX=phones_index

# Redis
REDIS_PASSWORD= # if using auth
