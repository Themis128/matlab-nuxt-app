{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": [
    "nuxt.config.ts",
    "app.vue",
    "error.vue",
    "pages/**/*.{js,ts,vue}",
    "server/**/*.{js,ts}",
    "plugins/**/*.{js,ts}",
    "components/**/*.vue",
    "composables/**/*.{js,ts}",
    "utils/**/*.{js,ts}",
    "stores/**/*.{js,ts}",
    "types/**/*.{js,ts}",
    "domain/**/*.{js,ts}",
    "application/**/*.{js,ts}"
  ],
  "project": [
    "**/*.{js,ts,vue,mjs,cjs}",
    "!**/node_modules/**",
    "!**/.nuxt/**",
    "!**/.output/**",
    "!**/dist/**",
    "!**/build/**",
    "!**/coverage/**",
    "!**/.git/**",
    "!**/venv/**",
    "!**/.venv/**",
    "!**/__pycache__/**",
    "!**/lancedb_data/**",
    "!**/test-results/**",
    "!**/playwright-report/**"
  ],
  "ignore": [
    "**/*.test.{js,ts}",
    "**/*.spec.{js,ts}",
    "**/__tests__/**",
    "**/tests/**",
    "scripts/**",
    "tools/**",
    "public/tools/**",
    "mcp-servers/**",
    "python_api/**",
    "python/**",
    "matlab/**",
    "examples/**",
    "docs/**",
    "infrastructure/**",
    "data/**",
    "lancedb_data/**",
    "logs/**",
    ".vscode/**",
    ".cursor/**",
    ".goosy/**",
    "i18n.config.ts",
    "assets/css/main.css",
    "public/assets/css/main.css"
  ],
  "ignoreDependencies": [
    "@nuxt/devtools",
    "@sentry/cli",
    "vue-tsc",
    "autoprefixer",
    "postcss",
    "@myuon/refactor-mcp",
    "vue",
    "h3",
    "@algolia/client-search",
    "@algolia/transporter",
    "@sentry/nuxt",
    "lucide-vue-next",
    "primeicons",
    "vue-chartjs",
    "chart.js",
    "pinia-plugin-persistedstate",
    "better-sqlite3"
  ],
  "ignoreBinaries": [
    "pwsh",
    "yamllint",
    "cline",
    "snyk"
  ],
  "ignoreExportsUsedInFile": true
}
